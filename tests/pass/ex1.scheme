(module ex1
  (class (Element el))
  
  (class (Trans e1 e2))
  
  (class (Graph trans)
    (def (append t)
         (= idx (+ 1 (maximum (map fst @trans))))
         (= tmp (Graph (: (idx t) @trans)))))
  
  (class (Point x y z, asdf)  
    (def (add (Point x y z)
              (Point (+ @x x)
                     (+ @y y)
                     (+ @z z))))
    
    (def (min (Point x y z)
              (Point (- @x x)
                     (- @y y)
                     (- @z z))))
    
    (def (distance)       
         (Point (sqrt (+ @x x)
                      (- @y y)
                      (- @z z))))  
    )
  
  (class (Foo state)
    (def (isOn)
         ((switch isOn)
          (case "On" true)
          (case "on" true)
          (case "Off" false)
          (case "off" false)
          (default "state not found"))))   
  )



